{
  "name": "gooder.work",
  "version": "1.0.0",
  "description": "Do gooder work",
  "author": "Phillip Malboeuf & Myles Tan",
  "scripts": {
    "dev": "parcel src/index.html -p 8080 --open",
    "dev:server": "parcel watch src/server.tsx --target=node --out-file=server.js | env $(cat .env) nodemon dist/server.js",
    "build": "parcel build src/server.tsx --target=node --out-file=server.js | parcel build src/index.tsx --experimental-scope-hoisting",
    "test": "parcel watch tests/*.test.tsx | jest --watchAll",
    "lint": "tslint -c tslint.json 'src/**/*.ts{,x}'"
  },
  "dependencies": {
    "algoliasearch": "^3.33.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "common-tags": "^1.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^3.2.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-instantsearch-dom": "^5.7.0",
    "react-router-dom": "^5.0.1",
    "stripe": "^7.4.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/algoliasearch": "^3.30.12",
    "@types/body-parser": "^1.17.0",
    "@types/common-tags": "^1.8.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/mongodb": "^3.1.28",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-instantsearch-dom": "^5.2.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/stripe": "^6.30.8",
    "@types/styled-components": "^4.1.16",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "parcel-bundler": "^1.12.3",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  }
}
